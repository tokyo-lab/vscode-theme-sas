{
  "name": "dark-sas",
  "displayName": "dark sas",
  "description": "dark theme for sas language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sas",
        "aliases": [
          "SAS",
          "sas"
        ],
        "extensions": [
          ".sas"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sas",
        "scopeName": "source.sas",
        "path": "./syntaxes/sas.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "dark sas",
        "uiTheme": "vs-dark",
        "path": "./themes/dark sas-color-theme.json"
      }
    ]
  }
}
