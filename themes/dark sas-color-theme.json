{
  "name": "dark sas",
  "include": "./monokai-dimmed.json",
  "colors": {
    "editor.background": "#263238",
    "editor.foreground": "#eeffff",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["global.comment.line.sas", "global.comment.block.sas"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#3c9d38"
      }
    },
    {
      "name": "Global Statement",
      "scope": ["global.statement.sas"],
      "settings": {
        "foreground": "#2458dc"
      }
    },
    {
      "name": "Macro Variable",
      "scope": ["macro.variable.sas"],
      "settings": {
        "foreground": "#d89400"
      }
    },
    {
      "name": "Macro Routines",
      "scope": ["macro.routine.sas"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Macro Statements",
      "scope": ["macro.statement.sas"],
      "settings": {
        "foreground": "#7d2fb1"
      }
    },
    {
      "name": "Macro Functions",
      "scope": ["macro.function.sas"],
      "settings": {
        "foreground": "#a72573"
      }
    },
    {
      "name": "Server Connect",
      "scope": ["server.connect.sas"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Procedures",
      "scope": ["procedure.sas"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Procedure Options",
      "scope": ["procedure.option.sql.sas"],
      "settings": {
        "foreground": "#f071ca"
      }
    },
    {
      "name": "Procedures SQL",
      "scope": ["procedure.sql.sas"],
      "settings": {
        "foreground": "#864447"
      }
    },
    {
      "name": "Procedure SQL Alter Table",
      "scope": ["procedure.sql.alter-table.sas"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Procedure SQL Select",
      "scope": ["procedure.sql.select.sas"],
      "settings": {
        "foreground": "#ef2731"
      }
    },
    {
      "name": "Procedure SQL Clause",
      "scope": ["procedure.sql.clause.sas"],
      "settings": {
        "foreground": "#82ffc3"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": [
        "operator.arithmetic.sas",
        "operator.other.sas",
        "operator.logical.sas",
        "operator.sql.sas"
      ],
      "settings": {
        "foreground": "#ff5dd1"
      }
    },
    {
      "name": "String",
      "scope": ["global.string.sas"],
      "settings": {
        "foreground": "#e96d1a"
      }
    },
    {
      "name": "Statement",
      "scope": ["statement.sql.sas"],
      "settings": {
        "foreground": "#dbf76c"
      }
    },
    {
      "name": "Format and Inromats",
      "scope": [
        "format-informat.$w.sas",
        "format-informat.$wd.sas",
        "format-informat.w.sas",
        "format-informat.wd.sas",
        "format_informat.format.$w.sas",
        "format_informat.format.$wd.sas",
        "format_informat.format.w.sas",
        "format_informat.format.wd.sas",
        "format_informat.format.wp.sas",
        "format_informat.format.xw.sas",
        "format_informat.informat.$w.sas",
        "format_informat.informat.d.sas",
        "format_informat.informat.w.sas",
        "format_informat.informat.wd.sas",
        "format_informat.custom.sas"
      ],
      "settings": {
        "foreground": "#8de8dd"
      }
    },
    {
      "name": "System Options",
      "scope": ["system.options.sas"],
      "settings": {
        "foreground": "#ef00d7"
      }
    },
    {
      "name": "Global Keywords",
      "scope": ["global.keyword.sas"],
      "settings": {
        "foreground": "#204ba8"
      }
    },
    {
      "name": "Global Numeric Constant",
      "scope": ["global.numeric.constant.sas"],
      "settings": {
        "foreground": "#1b7638"
      }
    },
    {
      "name": "Functions",
      "scope": ["function.sas"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Statement",
      "scope": ["statement.sas"],
      "settings": {
        "foreground": "#774631"
      }
    }
  ]
}
